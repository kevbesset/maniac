{
  "name": "maniac",
  "version": "1.0.0",
  "description": "Enhance Vue.js projects effortlessly with maniac: a curated collection of components, CLI-powered simplicity, seamless integration, style autonomy and total ownership.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "bin": {
    "maniac": "./bin/index.js"
  },
  "dependencies": {
    "vue": "^3.4.23"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "fs-extra": "^11.2.0",
    "npm-run-all2": "^6.1.2",
    "ora": "^8.0.1",
    "prettier": "^3.2.5",
    "project-version": "^2.0.0",
    "prompts": "^2.4.2",
    "sass": "^1.75.0",
    "typescript": "~5.4.5",
    "vite": "^5.2.10",
    "vue-tsc": "^2.0.13"
  }
}
